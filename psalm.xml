<?xml version="1.0"?>
<psalm
    totallyTyped="true"
    ensureArrayStringOffsetsExist="true"
    resolveFromConfigFile="false"
>
    <plugins>
        <pluginClass class="Psalm\PhpUnitPlugin\Plugin"/>
    </plugins>
    <projectFiles>
        <directory name="."/>
        <ignoreFiles>
            <directory name="cache"/>
            <directory name="vendor"/>
            <directory name="vendor-bin"/>
        </ignoreFiles>
    </projectFiles>
</psalm>
